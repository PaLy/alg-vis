<AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="-1.0" prefWidth="-1.0" xmlns:fx="http://javafx.com/fxml" fx:controller="algvis2.ui.AlgVisFXMLController">
  <children>
    <VBox layoutX="0.0" layoutY="0.0" prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <HBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" spacing="10.0">
          <children>
            <TextField fx:id="inputField" prefHeight="-1.0" prefWidth="100.0" />
            <Button fx:id="buttonInsert" mnemonicParsing="false" onAction="#insertPressed" text="%button-push" />
            <Button fx:id="buttonDelete" mnemonicParsing="false" onAction="#deletePressed" text="%button-pop" />
            <Separator orientation="VERTICAL" prefHeight="-1.0" />
            <CheckBox fx:id="buttonPause" mnemonicParsing="false" selected="false" text="%button-pause" />
            <Button fx:id="buttonPrevious" disable="true" mnemonicParsing="false" onAction="#previousPressed"
                    text="%previous" />
            <Button fx:id="buttonNext" disable="true" mnemonicParsing="false" onAction="#nextPressed" text="%next" />
          </children>
          <padding>
            <Insets bottom="20.0" fx:id="x1" />
          </padding>
        </HBox>
        <HBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" spacing="10.0">
          <children>
            <Button fx:id="buttonClear" mnemonicParsing="false" onAction="#clearPressed" text="%button-clear" />
            <ChoiceBox id="button" fx:id="button_layout">
              <items>
                <FXCollections fx:factory="observableArrayList">
                  <!--<Layout fx:constant="BIN_TREE_LAYOUT" />-->
                  <!--<Layout fx:constant="LEFT_BIN_TREE_LAYOUT" />-->
                  <!--<Layout fx:constant="RIGHT_BIN_TREE_LAYOUT" />-->
                  <String fx:value="CompactLayout" />
                </FXCollections>
              </items>
            </ChoiceBox>
          </children>
          <padding>
            <Insets />
          </padding>
        </HBox>
      </children>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </VBox>
  </children>
</AnchorPane>
